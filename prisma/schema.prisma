generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Software {
  id        Int    @id @default(autoincrement())
  name      String @unique
  createdAt String
}

model Problem {
  id   Int    @id @default(autoincrement())
  name String
}

model Solution {
  id   Int    @id @default(autoincrement())
  name String
}
